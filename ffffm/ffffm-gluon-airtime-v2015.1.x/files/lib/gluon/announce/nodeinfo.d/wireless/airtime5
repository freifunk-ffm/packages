function file_exists(name)
 local f=io.open(name,"r")
 if f~=nil then
   io.close(f)
   return true
 else
   return false
 end
end

if (file_exists('/tmp/act5')) then
  local n = 0
  local act = util.trim(fs.readfile('/tmp/act5'))
  n = tonumber(act)
  local bus = util.trim(fs.readfile('/tmp/bus5'))
  m = tonumber(bus)
  string.format("%.2f", n / m)
  return (m / n)
end