function file_exists(name)
 local f=io.open(name,"r")
 if f~=nil then
   io.close(f)
   return true
 else
   return false
 end
end

if (file_exists('/tmp/act2')) then
  local n = 0
  local act = util.trim(fs.readfile('/tmp/act2'))
  n = tonumber(act)
  local bus = util.trim(fs.readfile('/tmp/bus2'))
  m = tonumber(bus)
  string.format("%.2f", m / m)
  return (n / m)
end
